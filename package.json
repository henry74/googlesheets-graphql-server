    "googleapis": "^40.0.0",
    "graphql": "^14.3.1",
    "graphql-middleware": "^3.0.2",
    "graphql-type-json": "^0.3.0",
    "lodash": "^4.17.13",
    "lodash.zipobject": "^4.1.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-typescript-server": "^0.18.2",
    "nodemon": "^1.19.1",
